_G.UILabels = {}
_G.ContentFrames = {}
_G.TabButtons = {}

function GetText(key)
    local lang = _G.Ghostz.Settings.Language or "English"
    if _G.Translations[lang] and _G.Translations[lang][key] then
        return _G.Translations[lang][key]
    end
    return _G.Translations["English"][key] or key
end

function SaveSettings()
    local json = HttpService:JSONEncode(_G.Ghostz.Settings)
    writefile("Ghostz_Config.json", json)
end

function LoadSettings()
    if isfile("Ghostz_Config.json") then
        local json = readfile("Ghostz_Config.json")
        local success, data = pcall(function() 
            return HttpService:JSONDecode(json) 
        end)
        if success then
            for k, v in pairs(data) do
                _G.Ghostz.Settings[k] = v
            end
        end
    end
end

pcall(LoadSettings)
--[[    Ghostz Hub v0.0.30 - Section 2 of 8    ]]

_G.ScreenGui = Instance.new("ScreenGui")
_G.ScreenGui.Name = "Ghostz"
_G.ScreenGui.ResetOnSpawn = false
_G.ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

local parentSuccess = pcall(function()
    _G.ScreenGui.Parent = game:GetService("CoreGui")
end)

if not parentSuccess then
    _G.ScreenGui.Parent = LocalPlayer:WaitForChild("PlayerGui")
end

-- Confetti Animation Function
function CreateConfetti()
    for i = 1, 40 do
        local confetti = Instance.new("TextLabel")
        confetti.Text = "â– "
        confetti.Size = UDim2.new(0, 10, 0, 10)
        confetti.Position = UDim2.new(math.random(10, 90)/100, 0, 0.1, 0)
        confetti.BackgroundTransparency = 1
        confetti.TextColor3 = Color3.fromHSV(math.random(), 0.9, 1)
        confetti.TextSize = 20
        confetti.Font = Enum.Font.GothamBold
        confetti.Parent = _G.Main
        confetti.ZIndex = 50
        
        local targetY = 0.8 + (math.random() * 0.2)
        local rotation = math.random(-360, 360)
        
        local tween = TweenService:Create(confetti, TweenInfo.new(1.5 + math.random(), Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
            Position = UDim2.new(confetti.Position.X.Scale, 0, targetY, 0),
            Rotation = rotation,
            TextTransparency = 1
        })
        
        tween:Play()
        game:GetService("Debris"):AddItem(confetti, 2)
    end
end

