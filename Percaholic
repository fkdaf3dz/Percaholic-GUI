        ApplyTheme(theme)
        TransFill.BackgroundColor3 = _G.ThemeColors[theme].Primary
        SaveBtn.BackgroundColor3 = _G.ThemeColors[theme].Primary
        ResetBtn.BackgroundColor3 = Color3.fromRGB(
            math.max(_G.ThemeColors[theme].Primary.R * 255 - 20, 0),
            math.max(_G.ThemeColors[theme].Primary.G * 255 - 20, 0),
            math.max(_G.ThemeColors[theme].Primary.B * 255 - 20, 0)
        )
        ExportBtn.BackgroundColor3 = Color3.fromRGB(
            math.max(_G.ThemeColors[theme].Primary.R * 255 - 40, 0),
            math.max(_G.ThemeColors[theme].Primary.G * 255 - 40, 0),
            math.max(_G.ThemeColors[theme].Primary.B * 255 - 40, 0)
        )
        SaveSettings()
    end)
end

ThemeDropdown.MouseButton1Click:Connect(function()
    ThemeFrame.Visible = not ThemeFrame.Visible
    LangFrame.Visible = false
end)

local sliderDragging = false
local sliderConnection = nil

local function UpdateSlider(input)
    local relativeX = input.Position.X - TransTrack.AbsolutePosition.X
    local scale = math.clamp(relativeX / TransTrack.AbsoluteSize.X, 0, 1)
    
    _G.Ghostz.Settings.BackgroundTransparency = scale
    TransFill.Size = UDim2.new(scale, 0, 1, 0)
    TransKnob.Position = UDim2.new(scale, -10, 0.5, -10)
    TransTitle.Text = GetText("Transparency") .. ": " .. string.format("%.2f", scale)
    ApplyTransparency(scale)
    SaveSettings()
end

TransKnob.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        sliderDragging = true
        
        if sliderConnection then sliderConnection:Disconnect() end
        sliderConnection = UserInputService.InputChanged:Connect(function(changedInput)
            if sliderDragging and (changedInput.UserInputType == Enum.UserInputType.MouseMovement or changedInput.UserInputType == Enum.UserInputType.Touch) then
                UpdateSlider(changedInput)
            end
        end)
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        sliderDragging = false
        if sliderConnection then
            sliderConnection:Disconnect()
            sliderConnection = nil
        end
    end
end)

TransTrack.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        UpdateSlider(input)
    end
end)

EnableToggle.MouseButton1Click:Connect(function()
    _G.Ghostz.Settings.UIEnabled = not _G.Ghostz.Settings.UIEnabled
    SaveSettings()
    
    if _G.Ghostz.Settings.UIEnabled then
        EnableToggle.BackgroundColor3 = _G.ThemeColors[_G.Ghostz.Settings.Theme].Primary
        ToggleCircle:TweenPosition(UDim2.new(1, -24, 0.5, -11), Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.2, true)
        _G.Main.BackgroundTransparency = _G.Ghostz.Settings.BackgroundTransparency
    else
        EnableToggle.BackgroundColor3 = Color3.fromRGB(60, 60, 70)
        ToggleCircle:TweenPosition(UDim2.new(0, 2, 0.5, -11), Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.2, true)
        _G.Main.BackgroundTransparency = 1
    end
end)

