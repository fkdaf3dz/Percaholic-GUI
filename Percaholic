-- Animation Functions
function OpenHub()
    Island.Visible = false
    _G.Main.Visible = true
    
    _G.Main.Size = UDim2.new(0, 0, 0, 0)
    _G.Main.Position = UDim2.new(0.5, 0, 0.5, 0)
    
    local tween = TweenService:Create(_G.Main, TweenInfo.new(0.6, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
        Size = UDim2.new(0, 618, 0, 475),
        Position = UDim2.new(0.5, -309, 0.45, -237)
    })
    tween:Play()
    
    CreateConfetti()
end

function CloseHub()
    local tween = TweenService:Create(_G.Main, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
        Size = UDim2.new(0, 0, 0, 0),
        Position = UDim2.new(0.5, 0, 0.5, 0)
    })
    
    tween:Play()
    tween.Completed:Connect(function()
        _G.Main.Visible = false
        Island.Visible = true
    end)
end

-- Island Click to Open
Island.MouseButton1Click:Connect(OpenHub)
--[[    Ghostz Hub v0.0.30 - Section 3 of 8    ]]

function ApplyTheme(themeName)
    local theme = _G.ThemeColors[themeName] or _G.ThemeColors.Purple
    _G.Main:FindFirstChildOfClass("UIStroke").Color = theme.Primary
    Island.BackgroundColor3 = theme.Primary
    Island:FindFirstChildOfClass("UIStroke").Color = theme.Secondary
    _G.Main:FindFirstChildOfClass("ImageLabel").ImageColor3 = theme.Primary
    _G.NavBar.BackgroundColor3 = theme.Nav
    
    for name, btn in pairs(_G.TabButtons) do
        if name == _G.Ghostz.CurrentTab then
            btn.BackgroundColor3 = theme.Primary
        else
            btn.BackgroundColor3 = Color3.fromRGB(35, 35, 45)
        end
    end
    
    _G.LogoAccent.TextColor3 = theme.Accent
    
    if _G.ContentFrames["Home"] then
        local home = _G.ContentFrames["Home"]
        if home:FindFirstChild("WelcomeBox") then
            local sub = home.WelcomeBox:FindFirstChild("SubTitle")
            if sub then sub.TextColor3 = theme.Accent end
        end
    end
    
    if _G.ContentFrames["Pages"] then
        local pages = _G.ContentFrames["Pages"]
        if pages:FindFirstChild("PagesTitle") then
            pages.PagesTitle.TextColor3 = theme.Accent
        end
    end
    
    if _G.ContentFrames["Credits"] then
        local credits = _G.ContentFrames["Credits"]
        if credits:FindFirstChild("CreditsTitle") then
            credits.CreditsTitle.TextColor3 = theme.Accent
        end
    end
end

function ApplyTransparency(value)
    if _G.Ghostz.Settings.UIEnabled then
        _G.Main.BackgroundTransparency = value
    else
        _G.Main.BackgroundTransparency = 1
    end
end

function SwitchTab(tabName)
    _G.Ghostz.CurrentTab = tabName
    local theme = _G.ThemeColors[_G.Ghostz.Settings.Theme]
    for name, btn in pairs(_G.TabButtons) do
        if name == tabName then
            btn.BackgroundColor3 = theme.Primary
            btn.TextColor3 = Color3.new(1,1,1)
        else
            btn.BackgroundColor3 = Color3.fromRGB(35, 35, 45)
            btn.TextColor3 = Color3.fromRGB(180, 180, 180)
        end
    end
    
    for name, frame in pairs(_G.ContentFrames) do
        frame.Visible = (name == tabName)
    end
end

local TabNames = {"Home", "Pages", "Credits"}

for i, tabName in ipairs(TabNames) do
    local TabBtn = Instance.new("TextButton")
    TabBtn.Size = UDim2.new(0, 90, 0, 34)
    TabBtn.Position = UDim2.new(0, 250 + (i-1)*100, 0.5, -17)
    
    if tabName == "Home" then
        TabBtn.BackgroundColor3 = _G.ThemeColors[_G.Ghostz.Settings.Theme].Primary
        TabBtn.TextColor3 = Color3.new(1,1,1)
    else
        TabBtn.BackgroundColor3 = Color3.fromRGB(35, 35, 45)
        TabBtn.TextColor3 = Color3.fromRGB(180, 180, 180)
    end
    
    TabBtn.BackgroundTransparency = 0.2
    TabBtn.Text = tabName
    TabBtn.TextSize = 16
    TabBtn.Font = Enum.Font.GothamBold
    TabBtn.AutoButtonColor = false
    TabBtn.Parent = _G.NavBar
    
    local TabCorner = Instance.new("UICorner")
    TabCorner.CornerRadius = UDim.new(0, 8)
    TabCorner.Parent = TabBtn
    
    _G.TabButtons[tabName] = TabBtn
    
    TabBtn.MouseButton1Click:Connect(function()
        SwitchTab(tabName)
    end)
end

CloseBtn = Instance.new("TextButton") -- Global
CloseBtn.Size = UDim2.new(0, 34, 0, 34)
CloseBtn.Position = UDim2.new(1, -48, 0.5, -17)
CloseBtn.BackgroundColor3 = Color3.fromRGB(220, 50, 50)
CloseBtn.Text = "X"
CloseBtn.TextColor3 = Color3.new(1,1,1)
