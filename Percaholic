ScriptContent.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        local pos = input.Position
        
        if LangFrame.Visible then
            local absPos = LangFrame.AbsolutePosition
            local absSize = LangFrame.AbsoluteSize
            local onButton = (pos.X >= LangDropdown.AbsolutePosition.X and pos.X <= LangDropdown.AbsolutePosition.X + LangDropdown.AbsoluteSize.X and
                             pos.Y >= LangDropdown.AbsolutePosition.Y and pos.Y <= LangDropdown.AbsolutePosition.Y + LangDropdown.AbsoluteSize.Y)
            if not onButton and (pos.X < absPos.X or pos.X > absPos.X + absSize.X or pos.Y < absPos.Y or pos.Y > absPos.Y + absSize.Y) then
                LangFrame.Visible = false
            end
        end
        
        if ThemeFrame.Visible then
            local absPos = ThemeFrame.AbsolutePosition
            local absSize = ThemeFrame.AbsoluteSize
            local onButton = (pos.X >= ThemeDropdown.AbsolutePosition.X and pos.X <= ThemeDropdown.AbsolutePosition.X + ThemeDropdown.AbsoluteSize.X and
                             pos.Y >= ThemeDropdown.AbsolutePosition.Y and pos.Y <= ThemeDropdown.AbsolutePosition.Y + ThemeDropdown.AbsoluteSize.Y)
            if not onButton and (pos.X < absPos.X or pos.X > absPos.X + absSize.X or pos.Y < absPos.Y or pos.Y > absPos.Y + absSize.Y) then
                ThemeFrame.Visible = false
            end
        end
    end
end)
