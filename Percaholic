    OtherScriptsContent.Visible = true
    OtherScriptsBtn.BackgroundColor3 = _G.ThemeColors[_G.Ghostz.Settings.Theme].Primary
    OtherScriptsBtn.TextColor3 = Color3.new(1,1,1)
    MainScriptsBtn.BackgroundColor3 = Color3.fromRGB(35, 35, 45)
    MainScriptsBtn.TextColor3 = Color3.fromRGB(180, 180, 180)
end)

local originalApplyTheme = ApplyTheme
ApplyTheme = function(themeName)
    local theme = _G.ThemeColors[themeName] or _G.ThemeColors.Purple
    originalApplyTheme(themeName)
    
    if MainScriptsBtn and OtherScriptsBtn then
        if MainScriptsContent.Visible then
            MainScriptsBtn.BackgroundColor3 = theme.Primary
            MainScriptsBtn.TextColor3 = Color3.new(1,1,1)
            OtherScriptsBtn.BackgroundColor3 = Color3.fromRGB(35, 35, 45)
            OtherScriptsBtn.TextColor3 = Color3.fromRGB(180, 180, 180)
        else
            OtherScriptsBtn.BackgroundColor3 = theme.Primary
            OtherScriptsBtn.TextColor3 = Color3.new(1,1,1)
            MainScriptsBtn.BackgroundColor3 = Color3.fromRGB(35, 35, 45)
            MainScriptsBtn.TextColor3 = Color3.fromRGB(180, 180, 180)
        end
    end
end

local CreditsFrame = Instance.new("Frame")
CreditsFrame.Name = "Credits"
CreditsFrame.Size = UDim2.new(1, 0, 1, 0)
CreditsFrame.BackgroundTransparency = 1
CreditsFrame.Visible = false
CreditsFrame.Parent = _G.ContentContainer
_G.ContentFrames["Credits"] = CreditsFrame

local CreditsTitle = Instance.new("TextLabel")
CreditsTitle.Size = UDim2.new(1, 0, 0, 50)
CreditsTitle.Position = UDim2.new(0, 0, 0, 30)
CreditsTitle.BackgroundTransparency = 1
CreditsTitle.Text = GetText("Credits")
CreditsTitle.TextColor3 = _G.ThemeColors[_G.Ghostz.Settings.Theme].Accent
CreditsTitle.TextSize = 36
CreditsTitle.Font = Enum.Font.GothamBlack
CreditsTitle.Parent = CreditsFrame

local CreditsText = Instance.new("TextLabel")
CreditsText.Size = UDim2.new(1, -40, 0, 200)
CreditsText.Position = UDim2.new(0, 20, 0, 100)
CreditsText.BackgroundTransparency = 1
CreditsText.Text = "Ghostz Hub v" .. _G.Ghostz.Version .. "\n\nCreated by: percaholicz\n\n" .. GetText("ThankYou")
CreditsText.TextColor3 = Color3.fromRGB(255, 255, 255)
CreditsText.TextSize = 18
CreditsText.Font = Enum.Font.Gotham
CreditsText.TextWrapped = true
CreditsText.Parent = CreditsFrame

-- RightShift Toggle
UserInputService.InputBegan:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.RightShift then
        if _G.Main.Visible then
            CloseHub()
        else
            OpenHub()
        end
    end
end)

ApplyTheme(_G.Ghostz.Settings.Theme)
if not _G.Ghostz.Settings.UIEnabled then
    _G.Main.BackgroundTransparency = 1
end

print("Ghostz Hub v" .. _G.Ghostz.Version .. " loaded successfully!")
